<div class="banner">
  <div class="container">
    <h1>Oot the box</h1>
    <p>Things to consider, always think oot the box</p>
  </div>
</div>
<div class="articles-container">
  <div class="articles-grid">
    <% @articles.each do |article| %>
      <%= link_to article do %>
        <div class="article-card">
          <%= cl_image_tag article.photo.key, class: "article-img" %>
          <p><%= article.title %></p>
        </div>
      <% end %>
    <% end%>
  </div>
</div>
